/* pages/timing/timing.wxss */
.container{
    padding: 0;
}

.container .message{
    font-size: 34rpx;
}

.container .time{
    position: relative;
    width: 300rpx;
    height: 300rpx;
    margin-top: 30rpx;
    border-radius: 50%;
    background-color: aqua;
}

.container .time .timing{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.time .timing text{
    display: inline-block;
    font-size: 50rpx;
}

.container .money{
    margin-top: 30rpx;
}

.money .number{
    margin-top: 20rpx;
}